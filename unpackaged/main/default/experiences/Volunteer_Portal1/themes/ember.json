  "customCSS" : "/* tables background */\n.slds-th__action {\n    background-color: #eee !important;\n}\n\n/* main menu desktop style adjustments */\n.slds-button_icon-bare, .slds-button--icon-bare, .forceListViewManagerGrid th.sortable.ascending a.toggle:after {\ndisplay: none !important;\n}\n\n.slds-th__action {\ntext-transform: uppercase !important;\n}\n\n/* main menu desktop style adjustments */\nnav.lwc-59kp5sov61j a.lwc-59kp5sov61j, nav.lwc-59kp5sov61j button.lwc-59kp5sov61j {\n    padding: 10px 30px !important;\n    text-transform: uppercase;\n}\n/* grey box with padding */\n.mlf-volunteer-box {\npadding: 40px;\nmargin-top:40px;\nborder: 0px solid #000;\nwidth: 100%;\nbackground-color: #eeeeee;\n}\n/* remove list view picker from list views desktop */\n.slds-page-header_object-home.slds-page-header--object-home.slds-page-header.forceListViewManagerHeader {\n    display: none !important;\n}\n/* remove list view picker from list views mobile */\n.listTitleContainer.uiBlock, .forceCommunityRecordListStandard .listTitle {\n    display: none !important;\n}\n/* remove view all button from list views */\n.viewAllLink {\n    display: none !important;\n}\n/* remove list view actions on mobile */\n.forceCommunityActionsContainerMobile {\n    display: none !important;\n}\n/* remove list view actions drop down on desktop */\n.forceVirtualActionMarker.forceVirtualAction,\n.slds-dropdown-trigger.slds-dropdown-trigger_click.keyboardMode--trigger.keyboard--default-focus.forceListViewManagerGridColumnHeaderMenu {\n    display: none !important;\n}\n/* remove screen flow borders */\n.forceCommunityFlowCommunity {\n    border: none !important;\n}\n/* forgot your password on login form */\n.salesforceIdentityLoginForm2 .inverseTextColor {\n    color: rgba(0, 0, 0, 0.8) !important;\n}  \n/* list view minimum height */\n.forceCommunityRecordListDesktop.forceListViewManager .listDisplays {\n    min-height: 200px !important;\n}\n/* remove mobile list view swipe actions */\n.swipeBody {\n    display: none !important;\n}\n.slds-list--horizontal .slds-item--label {\n    width: 40% !important;\n}\n.slds-list--horizontal .slds-item--detail {\n    width: 60% !important; \n}\n\n@media only screen and (max-width: 600px) {\n    /* login button on mobile */\n    .comm-user-profile-menu__login-button[lwc-sf4l3atjv5] {\n        font-size: .75em;\n        padding: 0 10px;\n    }\n}\n/* table hinting on mobile */\n#mobile-tables-hint {\n background:#ff5e00;\n    text-align:center;\n    width: 100%; \npadding: 0.5em; \nmargin: 20px auto; \ncolor:white;\n    }\n\n@media (min-width:801px) {\n #mobile-tables-hint {\n    display:none;\n}}",
